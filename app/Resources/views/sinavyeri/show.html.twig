{% extends 'base.html.twig' %}

{% block body %}
    <h1>Sinavyeri</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sinavyeri.id }}</td>
            </tr>
            <tr>
                <th>Sinavyeri</th>
                <td>{{ sinavyeri.sinavyeri }}</td>
            </tr>
            <tr>
                <th>Adres</th>
                <td>{{ sinavyeri.adres }}</td>
            </tr>
            <tr>
                <th>Telefon</th>
                <td>{{ sinavyeri.telefon }}</td>
            </tr>
            <tr>
                <th>Yetkilikisi</th>
                <td>{{ sinavyeri.yetkilikisi }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('sinavyeri_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('sinavyeri_edit', { 'id': sinavyeri.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
